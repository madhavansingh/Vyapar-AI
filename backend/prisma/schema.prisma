generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Inventory {
  id        Int      @id @default(autoincrement())
  name      String   @unique       // ðŸ‘ˆ Important! Make `name` unique
  quantity  Int
  expiry    String?  // Optional field for expiry dates
  createdAt DateTime @default(now())
}

